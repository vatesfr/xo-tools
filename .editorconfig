# EditorConfig is awesome: http://EditorConfig.org

# top-most EditorConfig file
root = true

[*]
charset = utf-8
end_of_line = lf
insert_final_newline = true
trim_trailing_whitespaces = true


[*.js]
indent_size = 2
indent_style = space

[*.{md,mdwn,mdown,markdown}]
indent_size = 4
indent_style = space
